<body>

<h2>Vue demo</h2>

<div id="app">
  <ul>
    <li v-for="x in todos">
      {{ x.text }}
    </li>
  </ul>
</div>

<script>
myObject = new Vue({
  el: '#app',
  data: {
    todos: [
      { text: 'fries' },
      { text: 'fish' },
      { text: 'whatever else humans are supposed to eat' }
    ]
  }
})
</script>

</body>
</html>

